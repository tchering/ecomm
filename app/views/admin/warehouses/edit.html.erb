<div class="container mx-auto px-4 py-8">
  <div class="flex justify-between items-center mb-6">
    <h1 class="text-2xl font-bold text-gray-900">Edit Warehouse</h1>
    <div class="flex space-x-3">
      <%= link_to admin_warehouses_path, class: "px-4 py-2 bg-gray-100 rounded-md text-gray-700 hover:bg-gray-200 flex items-center" do %>
        <i class="fas fa-arrow-left mr-2"></i> Back to Warehouses
      <% end %>
      <%= link_to admin_warehouse_path(@warehouse), class: "px-4 py-2 bg-blue-100 rounded-md text-blue-700 hover:bg-blue-200 flex items-center" do %>
        <i class="fas fa-eye mr-2"></i> View Warehouse
      <% end %>
    </div>
  </div>
  <div class="bg-white shadow-md rounded-lg overflow-hidden">
    <div class="p-6">
      <%= render "form", warehouse: @warehouse %>
    </div>
  </div>
</div>
 